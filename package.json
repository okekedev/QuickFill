{
  "name": "quick-fill",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev-client": "expo start --dev-client"
  },

  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "buffer": "^6.0.3",
    "expo": "~53.0.0",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11", 
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "pdf-lib": "^1.17.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0"
  },
  "expo": {
    "name": "Quick Fill",
    "slug": "Quick Fill",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./icon.png",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "us.sundai.quickfill"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "package": "us.sundai.quickfill"
    }
  }
}
